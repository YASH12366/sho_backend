<h1>This is Product Index</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Price</th>
    <th>Price</th>
    <th></th>
  </tr>
 
  <% @products.each do |product| %>
    <tr>
      <td><%= product.title %></td>
      <td><%= product.price %></td>
      <td><%= product.inventory_count %></td>
      <td><%= link_to 'Show', product_path(product) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Product', new_product_path %>